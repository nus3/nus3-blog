{
  "name": "gatsby-starter-blog",
  "private": true,
  "description": "A starter for a blog powered by Gatsby and Markdown",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@fontsource/lato": "4.5.0",
    "@fontsource/noto-sans-jp": "4.5.0",
    "astro": "2.0.15",
    "autoprefixer": "10.3.6",
    "postcss": "8.3.8",
    "postcss-nested": "5.0.6",
    "prismjs": "1.25.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-icons": "4.2.0",
    "tailwindcss": "2.2.16"
  },
  "devDependencies": {
    "@types/react-helmet": "6.1.2",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.26.0",
    "prettier": "2.4.1",
    "typescript": "4.4.3"
  },
  "homepage": "https://github.com/nus3/nus3-blog#readme",
  "license": "0BSD",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nus3/nus3-blog.git"
  },
  "scripts": {
    "setup": "yarn --frozen-lockfile --ignore-optional",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:src": "eslint src --ext ts --ext tsx --max-warnings 0",
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --max-warnings 0",
      "prettier --write"
    ]
  }
}
